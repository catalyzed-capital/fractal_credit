{
    "$id": "https://cosmonic.com/concordance/mutualcreditaccount/WithdrawCredit.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "WithdrawCredit",
    "type": "object",
    "properties": {
      "accountNumber": {
        "type": "string",
        "description": "The account number"
      },
      "amount": {
        "type": "integer",
        "description": "The amount to withdraw"
      },
      "note": {
        "type": "string",
        "description": "An optional note to be associated with the withdrawal"
      },
      "entityId": {
        "type": "string",
        "description": "The ID of the entity performing the withdrawal"
      }     
    },
    "required": ["accountNumber", "amount", "entityId"]
  }
  