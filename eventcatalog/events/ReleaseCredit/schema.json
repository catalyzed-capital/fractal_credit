{
    "$id": "https://cosmonic.com/concordance/mutualcreditaccount/ReleaseCredit.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "ReleaseCredit",
    "type": "object",
    "description": "A request to release credit from a credit transfer",
    "properties": {
      "accountNumber": {
        "type": "string",
        "description": "The account number"
      },       
      "entityId": {
        "type": "string",
        "description": "The ID of the entity"
      },
      "creditTransferId": {
        "type": "string",
        "description": "A unique ID identifying the credit transfer transaction"
      }
    },
    "required": ["accountNumber", "entityId", "creditTransferId"]
  }
  