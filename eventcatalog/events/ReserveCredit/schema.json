{
    "$id": "https://cosmonic.com/concordance/mutualcreditaccount/ReserveCredit.json",
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "ReserveCredit",
    "type": "object",
    "properties": {
      "accountNumber": {
        "type": "string",
        "description": "The account number"
      },
      "amount": {
        "type": "integer",
        "description": "The amount to reserve"
      },    
      "entityId": {
        "type": "string",
        "description": "The ID of the entity performing the withdrawal"
      },
      "creditTransferId": {
        "type": "string",
        "description": "A unique ID identifying the credit transfer transaction"
      }
    },
    "required": ["accountNumber", "amount", "entityId", "creditTransferId"]
  }
  